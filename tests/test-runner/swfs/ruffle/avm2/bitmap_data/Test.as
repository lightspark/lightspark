function dumpBitmap(bitmap: BitmapData): void {
   var _loc2_: int = 0;
   while(_loc2_ < bitmap.width)
   {
      var _loc1_: int = 0;
      while(_loc1_ < bitmap.height)
      {
         trace("// bitmap.getPixel32(" + _loc2_ + ", " + _loc1_ + ")");
         trace(bitmap.getPixel32(_loc2_,_loc1_));
         _loc1_ = _loc1_ + 1;
      }
      _loc2_ = _loc2_ + 1;
   }
}

